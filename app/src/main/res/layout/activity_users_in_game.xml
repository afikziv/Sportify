<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UsersInGame">

    <ListView
        android:id="@+id/userListView"
        android:layout_width="410dp"
        android:layout_height="487dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="102dp"
        android:layout_marginBottom="143dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.151"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/joinButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/userListView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="161dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="-106dp"
        android:text="הצטרף"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="583dp" />

    <ImageView
        android:id="@+id/tennis"
        android:layout_width="61dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="301dp"
        android:layout_marginBottom="659dp"
        android:src="@drawable/tennis"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/soccer"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="293dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="53dp"
        android:layout_marginBottom="661dp"
        android:src="@drawable/soccer"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/basketball"
        android:layout_width="63dp"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="166dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="182dp"
        android:layout_marginBottom="661dp"
        android:src="@drawable/basketball"
        android:visibility="invisible" />
</RelativeLayout>